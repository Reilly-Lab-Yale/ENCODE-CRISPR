python /n/casa/src/wrap_peak_calling.py \
  /n/casa/analysis/CRISPR_WG/tcell_analysis/220304_TG2_GITR_KRAB__CASA_input__R1.txt \
  /n/casa/analysis/CRISPR_WG/tcell_analysis/220304_TG2_GITR_KRAB__CASA__R1__allPeaks \
  -b sabeti-encode \
  -g gs://haddath/sgosai/ \
  -ws 200 -ss 200 -z us* -p -j 50

python /n/casa/src/wrap_peak_calling.py \
